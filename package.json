{
  "name": "react-ab-tasty",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "description": "react-ab-tasty for easy A/B testing with dynamic rendering, persistent state, and logging",
  "author": "Dmytro Barabash (https://tutizaraz.com)",
  "homepage": "https://github.com/tutizaraz/react-ab-tasty",
  "keywords": [
    "react",
    "typescript",
    "ab test",
    "ab testing",
    "react-ab-test",
    "react-ab-tasty"
  ],
  "exports": {
    ".": {
      "import": "dist/index.es.js",
      "require": "dist/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "yarn build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
